<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pji.example.pji.bookset" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".AccueilActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".AjouterActivity"
            android:label="@string/title_activity_ajouter_element"
            android:parentActivityName=".AccueilActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="pji.example.pji.bookset.AccueilActivity" />
        </activity>
        <activity
            android:name=".AjouterElementActivity"
            android:label="@string/title_activity_ajouter_element_activity"
            android:parentActivityName=".AjouterActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="pji.example.pji.bookset.AjouterActivity" />
        </activity>
    </application>

</manifest>
